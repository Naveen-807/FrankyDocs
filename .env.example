# Google service account credentials:
# - Either a filepath to the JSON key OR inline JSON (stringified).
GOOGLE_SERVICE_ACCOUNT_JSON=

# Encryption key for wallet/private material (32 bytes).
# Accepts hex (64 chars) or base64.
DOCWALLET_MASTER_KEY=

# Local web server (join + approvals)
HTTP_PORT=8787
# Optional: public URL for approval links (e.g. an ngrok/Cloudflare tunnel). Defaults to http://localhost:$HTTP_PORT
PUBLIC_BASE_URL=

# Polling intervals
POLL_INTERVAL_MS=5000
DISCOVERY_INTERVAL_MS=60000

# Optional: limit discovery to a single doc id (recommended for first run)
DOCWALLET_DOC_ID=

# Optional: only discover docs whose name starts with this prefix (recommended)
DOCWALLET_NAME_PREFIX=[DocWallet]

# Set to 1 to discover all accessible Docs (use with caution)
DOCWALLET_DISCOVER_ALL=0

# Sui
# Used as the Sui HTTP fullnode RPC URL for DeepBookV3Client (example: https://fullnode.testnet.sui.io:443)
SUI_RPC_URL=
DEEPBOOK_ENABLED=0

# Arc
ARC_RPC_URL=https://rpc.testnet.arc.network
ARC_ENABLED=1
ARC_USDC_ADDRESS=0x3600000000000000000000000000000000000000

# Circle (Arc settlement via dev-controlled wallets)
CIRCLE_ENABLED=0
CIRCLE_API_KEY=
CIRCLE_ENTITY_SECRET=
# Optional; if empty, the agent creates a new wallet set
CIRCLE_WALLET_SET_ID=
# Circle blockchain id string (per Circle Wallets). Default aims at Arc testnet.
CIRCLE_BLOCKCHAIN=ARC-TESTNET
# "SCA" or "EOA" depending on Circle support for Arc
CIRCLE_ACCOUNT_TYPE=EOA

# ENS (optional)
ENS_RPC_URL=

# Yellow (optional)
YELLOW_ENABLED=0
# NitroRPC/0.4 clearnode RPC endpoint (required when YELLOW_ENABLED=1)
YELLOW_RPC_URL=
# Optional WebSocket endpoint (not used yet)
YELLOW_WS_URL=
# Optional application name used in auth_request
YELLOW_APP_NAME=DocWallet
